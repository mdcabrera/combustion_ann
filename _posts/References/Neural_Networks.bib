number = {June},
pages = {20--28},
pmid = {15315672},
title = {{Deep neural networks for segmentation .}},
url = {https://www.cv-foundation.org/openaccess/content{\_}cvpr{\_}workshops{\_}2015/W01/html/Brebisson{\_}Deep{\_}Neural{\_}Networks{\_}2015{\_}CVPR{\_}paper.html},
