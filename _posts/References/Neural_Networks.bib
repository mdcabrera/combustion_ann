keywords = {Artificial neural networks,Data-based modeling,Joint probability density function,Principal component analysis},
pages = {279--291},
publisher = {Elsevier Inc.},
title = {{A framework for data-based turbulent combustion closure: A posteriori validation}},
url = {https://doi.org/10.1016/j.combustflame.2019.08.039},
volume = {210},
year = {2019}
}
